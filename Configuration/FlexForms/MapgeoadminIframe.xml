<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Map geo admin / iframe</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.iframe.srcParam>
                        <TCEforms>
                            <label><![CDATA[iframe src params after ?lang=xx&...]]></label>
                            <config>
                                <type>input</type>
                                <size>255</size>
                            </config>
                        </TCEforms>
                    </settings.iframe.srcParam>
                    <settings.iframe.width>
                        <TCEforms>
                            <label>Width (px or %)</label>
                            <config>
                                <default>100%</default>
                                <type>input</type>
                                <size>10</size>
                            </config>
                        </TCEforms>
                    </settings.iframe.width>
                    <settings.iframe.height>
                        <TCEforms>
                            <label>Height (px or %)</label>
                            <config>
                                <default>300px</default>
                                <type>input</type>
                                <size>10</size>
                            </config>
                        </TCEforms>
                    </settings.iframe.height>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>
